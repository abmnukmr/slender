{
    "name": "slender/slender",
    "type": "library",
    "description": "Micro Application Framework",
    "keywords": ["app","framework","slim","slender"],
    "license": "MIT",
    "version": "0.0.0",
    "homepage": "http://alanpich.github.io/slender",
    "authors": [
        {
            "name": "Alan Pich",
            "email": "alan.pich@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "slim/slim": "dev-develop",
        "symfony/finder": "*",
        "symfony/yaml": "*",
        "symfony/class-loader": "2.4.2",
        "doctrine/annotations": "*",
        "twig/twig": "*"
    },
    "autoload": {
        "psr-4": {
            "Slender\\": "src/",
            "Slender\\Base\\": "core-modules/base",
            "Slender\\Module\\RouteManager\\": "core-modules/route-manager/src",
            "Slender\\Module\\Twig\\": "core-modules/twig/src",
            "Slender\\Module\\Controllers\\": "core-modules/controllers/src",
            "Slender\\Module\\EventManager\\": "core-modules/event-manager/src",
            "Slender\\Module\\DependencyInjector\\": "core-modules/dependency-injector/src",
            "Slender\\Module\\ServiceManager\\": "core-modules/service-manager/src"
        }
    },
    "require-dev": {
        "alebear/versioncontrol_git": "dev-master",
        "phpunit/phpunit": "3.7.32",
        "sebastian/phpcpd": "*",
        "phpmd/phpmd": "*",
        "squizlabs/php_codesniffer": "dev-master",
        "fabpot/php-cs-fixer": "*",
        "satooshi/php-coveralls": "dev-master",
        "mockery/mockery": "0.9.0"
    }
}
